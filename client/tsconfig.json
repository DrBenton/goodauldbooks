{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["ES2015", "ES2017", "dom"],
    "jsx": "react",
    "types": [],
    "baseUrl": "./src",
    "paths": {
      "*": ["../generated/*", "*"]
    }
  },
  "include": ["./src/**/*", "./generated/**/*"]
}
