{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "es2017", "dom"],
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "*": ["../generated/*", "*"]
    },
    "typeRoots": ["./node_modules/@types", "./@types"]
  },
  "include": ["./src/**/*", "./generated/**/*"]
}
