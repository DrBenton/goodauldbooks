{
  "extends": ["tslint:latest"],
  "rules": {
    "interface-name": [true, "never-prefix"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "object-literal-sort-keys": false,
    "no-empty-interface": false
  }
}
